  VALUES (staff_id = ?, restaurant_id = ?, first_name = ?, last_name = ?, position = ?, password = ?, SALT, contact_num = ?, email = ?)"

//Put for customer login
let query = "INSERT INTO customers(customer_id, first_name, last_name, password, salt, email)\
  VALUES (customer_id = ?m first_name = ?, last_name = ?, password = ?, salt, email = ?)"

